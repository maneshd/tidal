bpm x = do setcps (x / 60 / 4)

scene x = do (once $ s "bd" # n x # gain 0)

do
  bpm 80
  scene 0
  once $ s "ocean:3" # gain 1.2 # pan 0.9

once $ s "ocean:3" # gain 1.2 # pan 0.1

xfade 3
  $ s "KETHUK:0*2"
  # gain (range 0.85 1.15 $ slow 8 sine)
  # pan (range 0.3 0.7 $ slow 5 sine)
  # room 1.1

d2
  $ slow 4
  $ up "-6 0" ----------------------
  # s "sgong:1"
  # grain 0.05 2.0
  # room 1.1 # gain 0.9 # legato 1.0

d4
  $ slow 2
  $ echo 8 (1/8) 0.88
  $ n "<[1,3] [2,5]>"
  # s "saron" # legato 1.0 # room 1.01 # gain 0.75

d5
  $ slow 4
  $ n  "3  3 2 [2 2]"
  # up "0 -7 0 [0 5]"
  # s "[slenthem, sslenthem]"
  # legato 1.0 # room 1.01 # gain 0.80

do
  d1
    $ juxBy 0.8 (rev)
    $ superimpose (# up "12")
    $ often (# up "[-12,-7]")
    $ s "ocean:3"
    # legato 0.95
    # grain (range 0.0 0.99 rand) 0.2
    # gain (range 1.2 1.4 $ slow 8 sine)
  d4
    $ slow 2
    $ off (1/16) (|+ up "<5 <2 5 8>>")
    $ echo 8 (1/8) 0.88
    $ n "<[1,3] [2,5]>"
    # s "saron" # legato 1.0 # room 1.01 # gain 0.77

d5 $ silence

d4
  $ slow 2
  $ echo 8 (1/8) 0.90
  $ n "<[1,3] [2,5]>" # s "saron"
  # legato 1.0 # room 1.01 # gain 0.77

-- On weak beat
xfadeIn 4 8 $ silence



